:cascade "include/layout.tx"

: override content -> {
  
 <table>
  <tr>
   <th>email</th>
   <th>name</th>
   <th>school</th>
   <th>profile</th>
  </tr>
: for $students -> $student{
  <tr>
   <td><: $student.email:></td>
   <td><: $student.name:></td>
   <td><: $student.school:></td>
   <td><: $student.profile:></td>
  </tr>
: }
 </table>
  
:}
